server {

   listen				443 ssl;
   server_name			hkovac.42.fr;
   server_tokens		off;
   ssl_certificate		/etc/nginx/ssl/server-cert.crt;
   ssl_certificate_key	/etc/nginx/ssl/server-cert.key;
   ssl					on;
   ssl_protocols		TLSv1.3;
   ssl_ciphers			HIGH:!aNULL:!MD5;
   ssl_prefer_server_ciphers on;

}